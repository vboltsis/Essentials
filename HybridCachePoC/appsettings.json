{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Database=hybridcache;Username=hybridcache;Password=hybridcache123",
    "Redis": "localhost:6379"
  },
  "Redis": {
    "InstanceName": "HybridCachePoC:"
  },
  "Cache": {
    "MemoryCache": {
      "SizeLimit": 1000,
      "ExpirationScanFrequency": "00:05:00"
    },
    "RedisCache": {
      "ExpirationMinutes": 60,
      "SlidingExpirationMinutes": 15
    }
  },
  "HealthChecks": {
    "Redis": {
      "Timeout": "00:00:10"
    }
  }
}
